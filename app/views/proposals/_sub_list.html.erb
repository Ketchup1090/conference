<%
# Variables:
# * container => ActiveRecord instance that has #proposals, e.g., a User.
# * title => Title of the container, e.g., "user", or leave blank to guess based on container's class.
# * sorter => Display the sort toolbar? Defaults to false.

title  = local_assigns[:title]
sorter = local_assigns[:title]
%>

<%= render :partial => "proposals/sub_list_for_kind", :locals => {:kind => :sessions, :container => container, :title => title, :sorter => sorter} %>
<%= render :partial => "proposals/sub_list_for_kind", :locals => {:kind => :proposals, :container => container, :title => title, :sorter => sorter} %>
